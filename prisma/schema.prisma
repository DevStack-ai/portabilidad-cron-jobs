generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["relationJoins"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model ISOFT_INPUT {
  IDISOFT               Int       @id @default(autoincrement())
  FECHA_REGISTRO        DateTime? @default(now()) @db.DateTime(0)
  ORIGEN                Int?
  ESTADO_FTP            Int?      @default(0)
  FECHA_LISTO_ENVIARFTP DateTime? @db.DateTime(0)
  FECHA_ENVIADOFTP      DateTime? @db.DateTime(0)
  ENVIADO_ORACLE        Int?      @default(0)
  TRANSACTION_ID        String?   @db.VarChar(255)
  MSISDN                String?   @db.VarChar(255)
  ICCID                 String?   @db.VarChar(255)
  DONOR_OP              String?   @db.VarChar(255)
  USERID                String?   @db.VarChar(255)
  AREA                  String?   @db.VarChar(255)
  STATUS                String?   @db.VarChar(255)
  ADDED_ON              String?   @db.VarChar(255)
  PROCESSED_ON          String?   @db.VarChar(255)
  REMARKS               String?   @db.VarChar(255)
  PRE_POST              String?   @db.VarChar(50)
  CONTRACT_ID           String?   @db.VarChar(255)
  CONTRATO_GENERADO     Int?      @default(0)
  NUMBER_PORT           String?   @db.VarChar(255)
  TICKET                String?   @db.VarChar(255)
  ESTADO                String?   @db.VarChar(255)
  CREADO                String?   @db.VarChar(255)
  CANAL                 String?   @db.VarChar(255)
  ETAPA_ACTUAL          String?   @db.VarChar(255)
  AGENTE                String?   @db.VarChar(255)
  NOMBRE_DE_CLIENTE     String?   @db.VarChar(255)
  CEDULA                String?   @db.VarChar(255)
  DIRECCION_ENTREGA     String?   @db.VarChar(255)
  DIRECCION_CLIENTE     String?   @db.VarChar(255)
  IMEI                  String?   @db.VarChar(255)
  SERIE_DE_SIMCARD      String?   @db.VarChar(255)
  NOMBRE_DEL_PLAN       String?   @db.VarChar(255)
  EMAIL_DEL_CLIENTE     String?   @db.VarChar(255)
  TIPO_DE_PLAN          String?   @db.VarChar(255)
  TIPO_EQUIPO           String?   @db.VarChar(255)
  GRUPO_ETAPA_FIINAL    String?   @db.VarChar(255)
  ERROR                 Int?      @default(100)
  CONTRACT_ATTEMPTS     Int?      @default(0)
  s3_spn_path           String?   @db.VarChar(256)
  s3_front_document     String?   @db.VarChar(256)
  STEP                  Int?      @default(0)
  ENVIADO_ORACLE_FECHA  DateTime? @db.DateTime(0)
  paperless_message     String?   @db.Text
  s3_contract_path      String?   @db.VarChar(256)
  s3_invoice_path       String?   @db.VarChar(256)
  port_type_id          Int       @default(3)
  nip                   String?   @db.VarChar(256)
  error_oracle          String?   @db.Text
  OPM_MSGIDENTIFIER     String?   @db.VarChar(256)
  OPM_TIME_STAMP        DateTime? @db.DateTime(0)
  OPM_MESSAGE           String?   @db.MediumText
  nombre_vendedor       String?   @db.VarChar(500)
  cedula_vendedor       String?   @db.VarChar(100)
  firma_vendedor        String?   @db.LongText
  firma_cliente         String?   @db.LongText

  @@index([ENVIADO_ORACLE], map: "ISOFT_INPUT_ENVIADO_ORACLE_IDX")
  @@index([ESTADO_FTP], map: "ISOFT_INPUT_ESTADO_FTP_IDX")
  @@index([MSISDN], map: "ISOFT_INPUT_MSISDN_IDX")
  @@index([NUMBER_PORT], map: "ISOFT_INPUT_NUMBER_PORT_IDX")
  @@index([PRE_POST], map: "ISOFT_INPUT_PRE_POST_IDX")
  @@index([TRANSACTION_ID], map: "ISOFT_INPUT_TRANSACTION_ID_IDX")
  @@index([ERROR], map: "ERROR")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model ISOFT_INPUT_TEMP {
  TRANSACTION_ID       Int?
  MSISDN               String? @db.VarChar(255)
  ICCID                String? @db.VarChar(255)
  TRANSACTION_ID_ISOFT Int?

  @@index([TRANSACTION_ID, MSISDN, ICCID, TRANSACTION_ID_ISOFT], map: "ISOFT_INPUT_TEMP_TRANSACTION_ID_IDX")
  @@ignore
}

model MM_PICKUP_DATA {
  id                 Int       @id @default(autoincrement())
  idcaso             Int?
  telefonoWa         String?   @db.VarChar(255)
  flujo              String?   @db.VarChar(255)
  telefonoMigrar     String?   @db.VarChar(255)
  existePencloud     String?   @db.VarChar(255)
  fotoSim            String?   @db.VarChar(255)
  serieSimCard       String?   @db.VarChar(255)
  fecha_registro     DateTime? @default(now()) @db.DateTime(0)
  estado_extractor   Int?      @default(0)
  fecha_extractor    DateTime? @db.DateTime(0)
  url_foto_documento String?   @db.VarChar(1000)
  numero_cedula      String?   @db.VarChar(100)

  @@index([estado_extractor], map: "MM_PICKUP_DATA_estado_extractor_IDX")
  @@index([idcaso], map: "MM_PICKUP_DATA_idcaso_IDX")
  @@index([telefonoMigrar], map: "MM_PICKUP_DATA_telefonoMigrar_IDX")
}

